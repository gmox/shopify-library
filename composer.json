{
    "name": "wsnyc/shopify",
    "description": "A Shopify API connector",
    "type": "library",
    "authors": [
        {
            "name": "Ryon Hunter",
            "email": "ryon@websolutionsnyc.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "5.2.*",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "fzaninotto/faker": "^1.6.0",
        "symfony/var-dumper": "^3.2.2",
        "mockery/mockery": "^0.9.7",
        "phpdocumentor/phpdocumentor": "2.*",
        "sebastian/phpcpd": "2.*",
        "incraigulous/data-factories": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shopify\\": "src/Shopify/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/Concerns/",
            "tests/Fixtures/"
        ]
    }
}
